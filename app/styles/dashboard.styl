@import "common"

section#dashboard 
  width: inherit

  ul#zoom-controls
    list-style: none;
    right: 0;
    top: 50%;
    margin-top: -75px;
    position: absolute;
    z-index: 1000

  ul#zoom-controls .btn
    width: 50px;
    height: 50px;
    background: $mainBlack;

  ul#zoom-controls li
    border-bottom: 1px solid #ededed;

  ul#zoom-controls li:last-child
    border-bottom: none;

  ul#zoom-controls .btn:hover
    background: $standardGray;

  ul#zoom-controls .btn:active
    background: $focusRed;

  .row
    display: block
    margin-bottom: 20px;
    overflow-y: hidden;